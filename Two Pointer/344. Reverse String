class Solution {
    public void reverseString(char[] s) {
        int low = 0;
        int high = s.length-1;
        while(low<=high){


            if(low==high)
                break;
            char temp = s[low];
            s[low] = s[high];
            s[high] = temp;
            
            low++;
            high--;
        }
        
    }
    
}
